    "publish": {
        "CollectEditorial": {
            "extensions": [
                "mov",
                "mp4"
            ],
            "source_dir": ""
        },
        "CollectHierarchyInstance": {
            "shot_add_hierarchy": {
                "parents": {
                    "folder": "shots",
                    "project": "{project[name]}",
                    "sequence": "{_sequence_}"
                },
                "parents_path": "{project}/{folder}/{sequence}"
            },
            "shot_add_tasks": {},
            "shot_rename_search_patterns": {
                "_sequence_": "(\\d{4})(?=_\\d{4})",
                "_shot_": "(\\d{4})(?!_\\d{4})"
            },
            "shot_rename_template": "{project[code]}_{_sequence_}_{_shot_}"
        },
        "CollectInstances": {
            "custom_start_frame": 0,
            "subsets": {
                "audioMain": {
                    "extensions": [
                        "wav"
                    ],
                    "families": [
                        "clip"
                    ],
                    "family": "audio",
                    "keepSequence": false,
                    "version": 0
                },
                "referenceMain": {
                    "extensions": [
                        "mp4"
                    ],
                    "families": [
                        "clip"
                    ],
                    "family": "review",
                    "keepSequence": false,
                    "version": 0
                }
            },
            "timeline_frame_offset": 0,
            "timeline_frame_start": 900000
        },
        "CollectTextures": {
            "active": true,
            "color_space": [
                "sRGB",
                "Raw",
                "ACEScg"
            ],
            "enabled": true,
            "input_naming_groups": {
                "textures": [
                    "asset",
                    "shader",
                    "version",
                    "channel",
                    "color_space",
                    "udim"
                ],
                "workfile": [
                    "asset",
                    "filler",
                    "version"
                ]
            },
            "input_naming_patterns": {
                "textures": [
                    "^([^_.]+)_([^_.]+)_v([0-9]{3,})_([^_.]+)_({color_space})_(1[0-9]{3}).+"
                ],
                "workfile": [
                    "^([^.]+)(_[^_.]*)?_v([0-9]{3,}).+"
                ]
            },
            "main_workfile_extensions": [
                "mra"
            ],
            "other_workfile_extensions": [
                "spp",
                "psd"
            ],
            "texture_extensions": [
                "exr",
                "dpx",
                "jpg",
                "jpeg",
                "png",
                "tiff",
                "tga",
                "gif",
                "svg"
            ],
            "texture_families": [],
            "texture_subset_template": "textures{Subset}_{Shader}_{Channel}",
            "workfile_families": [],
            "workfile_subset_template": "textures{Subset}Workfile"
        },
        "ExtractThumbnailSP": {
            "ffmpeg_args": {
                "input": [
                    "-apply_trc gamma22"
                ],
                "output": []
            }
        },
        "ValidateSceneSettings": {
            "active": true,
            "check_extensions": [
                "exr",
                "dpx",
                "jpg",
                "jpeg",
                "png",
                "tiff",
                "tga",
                "gif",
                "svg"
            ],
            "enabled": true,
            "families": [
                "render"
            ],
            "optional": true,
            "skip_timelines_check": []
        }
    }
}